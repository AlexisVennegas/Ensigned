{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/contact",
      "dest": "/api/contact.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}

